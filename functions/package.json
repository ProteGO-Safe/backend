{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "mocha --recursive --require ts-node/register 'test/**/*.ts'",
    "build": "tsc",
    "serve": "npm run build && firebase functions:config:get > .runtimeconfig.json --project protego-fb-dev && firebase emulators:start --only functions --project protego-fb-dev",
    "shell": "npm run build && firebase functions:config:get > .runtimeconfig.json --project protego-fb-dev && firebase functions:shell --project protego-fb-dev",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@google-cloud/secret-manager": "^3.2.3",
    "@google-cloud/storage": "^5.7.0",
    "axios": "^0.21.1",
    "crypto-random-string": "^3.3.0",
    "csv-parse": "^4.14.2",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "js-sha256": "^0.9.0",
    "jsdom": "^16.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "netmask": "^1.0.6",
    "ssh2": "^0.8.9",
    "ssh2-streams": "^0.4.10",
    "superagent": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/jsdom": "^16.2.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/netmask": "^1.0.30",
    "@types/node": "^13.13.36",
    "@types/ssh2": "^0.5.46",
    "@types/superagent": "^4.1.10",
    "@types/uuid": "^7.0.4",
    "chai": "^4.2.0",
    "firebase-functions-test": "^0.1.6",
    "mocha": "^8.2.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.12.0",
    "typescript": "^3.9.7"
  },
  "private": true
}
