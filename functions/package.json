{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "test": "mocha --recursive --require ts-node/register 'test/**/*.ts'",
    "build": "tsc",
    "serve": "npm run build && firebase functions:config:get > .runtimeconfig.json --project protego-fb-dev && firebase emulators:start --only functions --project protego-fb-dev",
    "shell": "npm run build && firebase functions:config:get > .runtimeconfig.json --project protego-fb-dev && firebase functions:shell --project protego-fb-dev",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "preinstall": "npx npm-force-resolutions"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@google-cloud/logging-bunyan": "^3.0.1",
    "@google-cloud/pubsub": "2.8.0",
    "@google-cloud/secret-manager": "^3.2.3",
    "@google-cloud/storage": "^5.7.0",
    "@opencensus/core": "^0.1.0",
    "@opencensus/exporter-stackdriver": "^0.1.0",
    "axios": "^0.21.2",
    "bignumber.js": "^9.0.1",
    "bunyan": "^1.8.15",
    "crypto-random-string": "^3.3.0",
    "csv-parse": "^4.14.2",
    "deep-equal": "^2.0.5",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.0",
    "iconv-lite": "^0.6.2",
    "js-sha256": "^0.9.0",
    "jsdom": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "netmask": "^2.0.1",
    "node-forge": "1.0.0",
    "ssh2": "^1.5.0",
    "ssh2-streams": "^0.4.10",
    "superagent": "^6.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/chai": "^4.2.14",
    "@types/continuation-local-storage": "^3.2.2",
    "@types/deep-equal": "^1.0.1",
    "@types/jsdom": "^16.2.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/netmask": "^1.0.30",
    "@types/node": "^14.14.20",
    "@types/ssh2": "^0.5.46",
    "@types/superagent": "^4.1.10",
    "@types/uuid": "^8.3.0",
    "chai": "^4.2.0",
    "firebase-functions-test": "^0.2.3",
    "mocha": "^8.4.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "private": true
}
